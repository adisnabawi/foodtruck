
<div class="container">
    <form role="form">
      <label for="sel1">Select State:</label>
      <select class="form-control" id="sel1">
        <option>Kedah</option>
        <option>Perlis</option>
        <option>Perak</option>
        <option>Selangor</option>
      </select>
      </form>


<br>
<div id="selectedState"></div>
<script>
$( "select" )
  .change(function() {
    var str = "";
    $( "select option:selected" ).each(function() {
      str = $( this ).text();
      if(str == "Kedah")
      {
          str = '<div class="card card-block text-left"><h4 class="card-title">Kedah</h4><div class="card-body"><a href="http://4sq.com/1wNsElf" data-toggle="tooltip" title="Alor Star, Kedah, Malaysia">Good Guys Food Truck </a></div></div>'
      }
    });
    document.getElementById("selectedState").innerHTML = str;
  })
  .trigger( "change" );
</script>
</div>
